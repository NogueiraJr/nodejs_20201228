		{
			"name": "http://localhost:3000/products POST TEST",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var keys = pm.environment.get('keys');",
							"",
							"if (!keys) {",
							"    keys = [ \"01\", \"02\", \"03\" ];",
							"}",
							"",
							"var currentKey = keys.shift();",
							"pm.environment.set('key', currentKey);",
							"pm.environment.set('keys', keys);",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"exec": [
							"var keys = pm.environment.get('keys');",
							"",
							"if (keys && keys.length > 0) {",
							"    postman.setNextRequest('http://localhost:3000/products');",
							"} else {",
							"    postman.setNextRequest(null);",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"title\":\"Produto {{key}}\",\n    \"description\":\"Descrição do Produto {{key}}\",\n    \"slug\":\"slug-{{key}}\",\n    \"price\":\"123\",\n    \"active\":true,\n    \"tags\":[\"A\", \"B\", \"C\", \"D\"]\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
